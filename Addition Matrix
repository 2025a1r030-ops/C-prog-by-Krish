#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() { 
    int r, c; 
    scanf("%d %d", &r, &c); 
    int m1[r][c], m2[r][c], m3[r][c]; 
    
    //Read a matrix - 1 
    for(int i=0; i<r; i++) { 
        for(int j=0; j<c; j++) { 
            scanf("%d", &m1[i][j]);
        }
    } 
    
    //Read a matrix - 2 
    for(int i=0; i<r; i++) { 
        for(int j=0; j<c; j++) { 
            scanf("%d", &m2[i][j]);
        }
    } 
    
    //logic for two matrix to be added 
    for(int i=0; i<r; i++) { 
        for(int j=0; j<c; j++) { 
            m3[i][j] = m1[i][j] + m2[i][j];
        }
    }  
    
    //print the result matrix 
    for(int i=0; i<r; i++) { 
        for(int j=0; j<c; j++) { 
            printf("%d ", m3[i][j]);
        } 
        printf("\n");
    }  
    
    


    
    return 0;
}
