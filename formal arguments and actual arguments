#include <stdio.h> 
//change in formal address 
void show(int *a, int *b) { 
    printf("a = %d b = %d \n", *a, *b); 
    *a = 100; 
    *b = 200; 
    printf("a = %d b = %d \n", *a, *b);  

}  
int main() { 
    int x = 10, y = 20; 
    show(&x, &y); 
    printf("x= %d y = %d \n", x, y);  
    return 0;
}
