//function with no parameters but have return type
#include <stdio.h> 

//function declaration
int getfactorial(void); 

//function calling 
int main() { 
int result = getfactorial(); 
printf("Factorial is %d", result); 
return 0; 
}  

//function definition 
int getfactorial(void) { 
    int num; 
    printf("Enter a number : "); 
    scanf("%d", &num); 
    int fact = 1; 
    for(int i=1; i<=num; i++) { 
        fact *= i; 
    } 
    return fact; 
}
