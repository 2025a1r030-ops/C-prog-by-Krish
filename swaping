#include <stdio.h> 

int main() { 
    int a, b, c; 
    printf("enter a :"); 
    scanf("%d", &a); 
    printf("enter b :"); 
    scanf("%d", &b); 

    //swap 
         c = a; 
         a = b; 
         b = c; 
    printf("after swap a : %d \n", a); 
    printf("after swap b : %d \n", b); 

return 0;
}
