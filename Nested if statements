#include <stdio.h> 
#include <ctype.h> 

int main() {
    char ch; 
    printf("Enter character: "); 
    scanf("%c", &ch); 
    if(isalpha (ch)) {
    ch = toupper(ch); 
    if(ch=='A' || ch=='E' || ch=='I' || ch=='O' || ch=='U') { 
        printf("%c is vowel", ch); 
    } 
    else { 
        printf("It is not vowel"); 
    } 
 }
     else { 
        printf("Invalid Input !!!!"); 
    }
    

return 0;
}  
