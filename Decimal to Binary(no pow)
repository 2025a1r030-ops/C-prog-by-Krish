#include <stdio.h> 

int main() { 
    int n, sum = 0, base = 1; 
    printf("Enter number: "); 
    scanf("%d", &n); 

    int n1 = n; 

    while(n != 0){ 
        int rem = n%10; 
        sum += rem * base; 
        base = 2 * base; 
        n /= 10;
} 

printf("Decimal of %d is %d", n1, sum);


    return 0;
}
