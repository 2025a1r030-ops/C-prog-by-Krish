#include <stdio.h> 
int main() { 
    int size; 
    printf("Array size : "); 
    scanf("%d", &size); 
    int stu[size];
    //reading the elements of an array 
    printf("Enter roll number of %d students : ", size); 
    for(int i=0; i<5; i++) { 
        scanf("%d", &stu[i]); 
    }  
    //displaying the elements of an array 
    for(int i=0; i<5; i++) { 
        printf("%d \t", stu[i]); //when size is not mentioned
    }  
     
    return 0; 
}
