#include <stdio.h> 

int main() { 
    int num, flag = 0; 
    int i=2;
     printf("Enter number : ");
    scanf("%d", &num); 
    while(i<= num/2) { 
        if(num%i == 0) { 
            flag = 1;
            break;
        } 
        i++; 
    } 
    if(flag == 0) 
    printf("Prime"); 
    else 
    printf("Not prime");
    return 0; 
    }
